<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xmas_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/citp"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Map"
            android:label="@string/title_activity_map" >
            <intent-filter>
                <action android:name="android.intent.action.MAP" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EventSchedule"
            android:label="@string/title_activity_event_schedule" >
            <intent-filter>
                <action android:name="android.intent.action.EVENT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".VisitorInfo"
            android:label="@string/title_activity_vistor_info" >
            <intent-filter>
                <action android:name="android.intent.action.VISTORINFO" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetInvolved"
            android:label="@string/title_activity_get_involved" >
            <intent-filter>
                <action android:name="android.intent.action.GETINVOLVED" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Sponsors"
            android:label="@string/title_activity_sponsors" >
            <intent-filter>
                <action android:name="android.intent.action.SPONSORS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vote"
            android:label="@string/title_activity_vote" >
            <intent-filter>
                <action android:name="android.intent.action.VOTE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FAQS"
            android:label="@string/title_activity_faqs" >
            <intent-filter>
                <action android:name="android.intent.action.FAQS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Parkingdirection"
            android:label="@string/title_activity_parkingdirection" >
            <intent-filter>
                <action android:name="android.intent.action.PARKINGDIRECTION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
