<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/snowbackground"
    tools:context="${relativePackage}.${activityClass}" >
    

    <Button
        android:id="@+id/FAQsButton"
        style="@style/bsOrnamentGreen"
        android:layout_alignTop="@+id/ParkingButton"
        android:layout_toLeftOf="@+id/ParkingButton"
        android:layout_toStartOf="@+id/ParkingButton"
        android:text="FAQs"
        android:textStyle="bold" />
    <Button
        android:id="@+id/ParkingButton"
        style="@style/bsOrnamentGreen"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="55dp"
        android:text="@string/parking"
        android:textStyle="bold" />

    <Button
        android:id="@+id/StoresButton"
        style="@style/bsOrnamentGreen"
        android:layout_alignTop="@+id/ParkingButton"
        android:layout_toRightOf="@+id/ParkingButton"
        android:text="Stores"
        android:textStyle="bold" />

    <Button
        android:id="@+id/EatsTreatButton"
        style="@style/bsOrnamentGreen"
        android:layout_below="@+id/ParkingButton"
        android:layout_marginLeft="17dp"
        android:layout_toRightOf="@+id/PartnerButton"
        android:text="@string/eats"
        android:textStyle="bold" />

    <Button
        android:id="@+id/PartnerButton"
        style="@style/bsOrnamentGreen"
        android:layout_alignLeft="@+id/FAQsButton"
        android:layout_alignTop="@+id/EatsTreatButton"
        android:layout_marginLeft="37dp"
        android:text="Partners"
        android:textStyle="bold" />
	
</RelativeLayout>
